  "scripts": {
    "watch": "ELEVENTY_ENV=dev npx eleventy --serve",
    "watch-silent": "ELEVENTY_ENV=dev npx eleventy --serve --quiet",
    "build": "ELEVENTY_ENV=prod eleventy",
    "debug": "ELEVENTY_ENV=dev DEBUG=* npx eleventy",
    "test": "ELEVENTY_ENV=dev npx eleventy --dryrun",
    "win-watch": "set ELEVENTY_ENV=dev & npx eleventy --serve",
    "win-build": "set ELEVENTY_ENV=prod & npx eleventy",
    "win-debug": "set ELEVENTY_ENV=dev DEBUG=* & npx eleventy",
    "win-test": "set ELEVENTY_ENV=dev & npx eleventy --dryrun"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "js-beautify": "^1.13.13",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-multimd-table": "^4.1.0"
  }
